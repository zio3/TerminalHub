{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SessionSettings": {
    "DefaultCols": 120,
    "DefaultRows": 30,
    "MaxSessions": 10,
    "BufferSize": 4096,
    "MaxBufferSize": 2000,
    "EnableBackgroundBuffering": true
  },
  "DevelopmentMode": {
    "ShowDevButton": true,
    "ShowDevWindowOnStartup": false
  },
  "ExternalTools": {
    "ClaudeCmdPath": null,
    "UseClaudeCode": true,
    "UseGeminiCli": true
  }
}
